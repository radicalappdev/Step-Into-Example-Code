#usda 1.0
(
    customLayerData = {
        string creator = "Reality Composer Pro Version 2.0 (494.40.1)"
    }
    defaultPrim = "Root"
    metersPerUnit = 1
    upAxis = "Y"
)

def Xform "Root"
{
    def RealityKitTimeline "Rocket"
    {
        reorder nameChildren = ["Track", "Track2"]
        float2 ui:nodegraph:node:pos = (20, 20)
        int ui:nodegraph:node:stackingOrder = 1

        def RealityKitTrack "Track"
        {
            def RealityKitAction "Emphasize"
            {
                string actionKind = "emphasize"
                int blendLayer = 300
                double duration = 1
                uniform token info:id = "E85F04D8-8C62-477B-A343-F5B24304C122"
                token motionType = "spin"
                double startTime = 0
                token style = "playful"
                rel target = </Root/ToyRocket>
            }
        }

        def RealityKitTrack "Track2"
        {
            def RealityKitAction "Emphasize"
            {
                string actionKind = "emphasize"
                int blendLayer = 300
                double duration = 1
                uniform token info:id = "75751CDE-89F6-4D01-98C2-E3C7F46914B6"
                token motionType = "float"
                double startTime = 0
                rel target = </Root/ToyRocket>
            }
        }
    }

    def "ToyRocket" (
        active = true
        references = @ToyRocket.usdz@
