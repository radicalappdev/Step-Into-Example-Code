        {
            rel behaviors = </Root/ToyRocket/OnNotification>
            uniform token info:id = "RCP.BehaviorsContainer"
        }

        def Preliminary_Behavior "OnNotification"
        {
            rel actions = </Root/ToyRocket/OnNotification/Action>
            uniform bool exclusive
            rel triggers = </Root/ToyRocket/OnNotification/Trigger>

            def Preliminary_Trigger "Trigger"
            {
                rel affectedObjects = </Root/ToyRocket>
                string identifier = "RunTimelines"
                token info:id = "Notification"
            }

            def Preliminary_Action "Action"
            {
                rel affectedObjects = </Root>
                rel animationLibraryKey = </Root/Rocket>
                rel animationLibraryKeyOverrideKey = </Root/ToyRocket/OnNotification/Trigger>
                token info:id = "PlayTimeline"
                int loops = 0
                int performCount = 1
                token type = "serial"
